<?xml version="1.0" encoding="gb2312" ?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd" >
<sqlMap>
	<!-- 按照角色的父节点查询 -->
	<insert id="system.addlog">
		insert into as_operlog (cip,cworkstation,iperson,doperate,cnode,cfunction,cresult,iinvoice,cplace)
		values
		(#cip#,#cworkstation#,#iperson#,#doperate#,#cnode#,#cfunction#,#cresult#,#iinvoice#,#cplace#)
	</insert>
</sqlMap>